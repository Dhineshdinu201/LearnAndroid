<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#0F2383" />
            <gradient android:angle="-90" android:startColor="#365FF4" android:endColor="#366CF4"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#3675F4" />
            <solid android:color="#365FF4"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#3675F4" />
            <gradient android:angle="-90" android:startColor="#3665F4" android:endColor="#3665F4" />
        </shape>
    </item>
</selector>